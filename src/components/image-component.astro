---
import ImageComponent from './image-component';
import getImgPath from './images';

interface ImageComponentProps {
  imgsrc: string;
}

interface Props {
  imgsrc: string | object;
}

const { imgsrc } = Astro.props;

const convertedImg = await getImgPath(imgsrc);
---

<ImageComponent client:visible imgsrc={convertedImg} />
